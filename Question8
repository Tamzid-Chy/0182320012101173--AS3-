import 'dart:io';
num add(num a, num b) {
  return a + b;
}
void main() {
  stdout.write("Enter the first number: ");
  num num1 = num.parse(stdin.readLineSync()!);
  stdout.write("Enter the second number: ");
  num num2 = num.parse(stdin.readLineSync()!);
  num sum = add(num1, num2);
  print("sum is $sum");
}
